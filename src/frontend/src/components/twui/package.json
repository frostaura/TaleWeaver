{
  "name": "twui",
  "version": "0.0.1",
  "description": "TaleWeaver UI Kit",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/twui/twui.esm.js",
  "exports": {
    ".": {
      "import": "./dist/twui/twui.esm.js",
      "require": "./dist/twui/twui.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/*": {
      "import": "./dist/*",
      "types": "./dist/*"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    },
    "./globalstore": {
      "import": "./dist/components/app/globalStore.js",
      "require": "./dist/components/app/globalStore.js",
      "types": "./dist/types/components/app/globalStore.d.ts"
    }
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.27.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.3.0"
  },
  "license": "MIT",
  "dependencies": {
    "@stencil/react-output-target": "^1.2.0",
    "@stencil/sass": "^3.2.2",
    "@stencil/store": "^2.2.0",
    "iconify-icon": "^3.0.0"
  }
}
